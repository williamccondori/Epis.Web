@{
    ViewBag.Title = "Objetivos educacionales";
}

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <h2>Objetivos educacionales</h2>
            <span>Estos son nuerstros objetivos educacionales:</span>
            <div class="panel-group" id="accordion">
                @for (int i = 0; i < 4; i++)
                {
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#elemento_@i">
                                    Este es el elemento @i
                                </a>
                            </h4>
                        </div>
                        <div id="elemento_@i" class="panel-collapse collapse">
                            <div class="panel-body">
                                Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut
                                labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco
                                laboris nisi ut aliquip ex ea commodo consequat.
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>